{
	"name": "df_hechos_llamadas",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "llamadas_tigo_csv",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_tipo",
						"type": "DatasetReference"
					},
					"name": "dimTipo"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_origen",
						"type": "DatasetReference"
					},
					"name": "dimOrigen"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_destino",
						"type": "DatasetReference"
					},
					"name": "dimDestino"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_fecha",
						"type": "DatasetReference"
					},
					"name": "dimFecha"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_hora",
						"type": "DatasetReference"
					},
					"name": "dimHora"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "cast1"
				},
				{
					"name": "lookup1"
				},
				{
					"name": "lookup2"
				},
				{
					"name": "lookup3"
				},
				{
					"name": "castFecha"
				},
				{
					"name": "lookup4"
				}
			],
			"scriptLines": [
				"source(output(",
				"          DESCRIPCION as string,",
				"          TIPO as string,",
				"          NUMERO_ORIGEN as string,",
				"          NUMERO_DESTINO as string,",
				"          FECHA as string,",
				"          HORA as string,",
				"          DURACION as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source(output(",
				"          id_tipo as integer,",
				"          tipo as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> dimTipo",
				"source(output(",
				"          id_origen as integer,",
				"          numero_origen as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> dimOrigen",
				"source(output(",
				"          id_destino as integer,",
				"          numero_destino as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> dimDestino",
				"source(output(",
				"          id_fecha as integer,",
				"          fecha as date,",
				"          {aÃ±o} as integer,",
				"          mes as integer,",
				"          mes_texto as string,",
				"          dia_texto as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> dimFecha",
				"source(output(",
				"          id_hora as integer,",
				"          hora as timestamp,",
				"          hora_texto as string,",
				"          hora24 as integer,",
				"          hora12 as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> dimHora",
				"source1 cast(output(",
				"          DURACION as integer",
				"     ),",
				"     errors: true) ~> cast1",
				"cast1, dimTipo lookup(source1@TIPO === dimTipo@tipo,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"lookup1, dimOrigen lookup(source1@NUMERO_ORIGEN == dimOrigen@numero_origen,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup2",
				"lookup2, dimDestino lookup(source1@NUMERO_DESTINO == dimDestino@numero_destino,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup3",
				"dimFecha cast(output(",
				"          fecha as string",
				"     ),",
				"     errors: true) ~> castFecha",
				"lookup3, dimHora lookup(source1@HORA == hora_texto,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup4"
			]
		}
	}
}